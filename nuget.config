<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <config>
        <add key="dependencyVersion" value="Lowest" />
        <add key="globalPackagesFolder" value="./packages" />
    </config>
    <packageSources>
        <clear />
        <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
        <add key="cluedin_public" value="https://pkgs.dev.azure.com/CluedIn-io/Public/_packaging/Public/nuget/v3/index.json" />
        <!-- <add key="cluedin_private" value="https://pkgs.dev.azure.com/CluedIn-io/_packaging/release/nuget/v3/index.json" /> -->
    </packageSources>
    <packageSourceCredentials>
    <cluedin_private>
        <add key="Username" value="using_azure_PAT" />
        <add key="ClearTextPassword" value="%CLUEDIN_NUGET_PAT%" />
    </cluedin_private>
</packageSourceCredentials>
</configuration>
